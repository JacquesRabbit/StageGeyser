<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="StageGeyser.MainWindow"
        Title="StageGeyser">

    <Panel Name="WindowPanel">
        <DockPanel Name="ControlBar" VerticalAlignment="Top" Height="50" Background="Gray">
            <DockPanel Name="ControlButtons" HorizontalAlignment="Right">
                <Button Name="OpenButton" Command="{Binding OpenFile}">
                    Open
                </Button>
            </DockPanel>
        </DockPanel>
    </Panel>


</Window>
